<div class="container-fluid">
  <div class="row justify-content-start align-items-end">
    <div class="col-sm-4">
      <p id="notice"><%= notice %></p>
      <h1>Договори</h1>
    </div> <!-- col -->

    <div class="col-sm-4">
      <%= list_change_buttons( "listChange('#{all_contracts_path}')",
                               "listChange('#{mine_contracts_path}')", 
                               @list_type[:all]==true ) 
      %>

    </div> <!-- col -->
  </div> <!-- row -->

  <div class="row">
    <div class="col-sm-12" id="data-container">

    <%= render 'contracts_table' %>
      
    </div> <!-- col -->
  </div> <!-- row -->


  <div class="row">
    <div class="col-sm-12">
      <pre> <%= link_to 'New Contract', new_contract_path %> </pre>
    </div> <!-- col -->
  </div> <!-- row -->

</div>